
{% extends "layout.twig" %}

{% block content %}

    <div class="featureimage">
          {{widget('feature', 'featurehome', {"header": "profile", "strapline":"profile", "buttonText":"Lorem Ipsum","defaultBgImg": asset('images/feature-bg.jpg'), 'positionTopPercentage': '50','showBtn': true})|raw}}
      <header class="bk-header">
          <div class="header-container wrapper clearfix">
            <div class="companyheader clearfix">
                  <div class="header-left">
                      {{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png')})|raw}}
                      {{widget('companyname', 'companyname', {"business": "profile"})|raw}}
                  </div>
                  <div class="header-right">
                      {{widget('extendednavigation', 'navigationheader', {})|raw}}
                      {{widget('ecombasket', 'ecombasket', {})|raw}}
                  </div>
            </div>
          </div>
      </header> 

      <div class="canvas wrapper clearfix">
              {{widget('ecombasket', 'ecombasket', {})|raw}}
              {{widget('ecomproductslist', 'ecomproductslist', {})|raw}}
              {{zone('main')|raw}}
      </div>
          
      <footer class="bk-footer">
              <div class="footer-container wrapper clearfix">
                  <div class="footer-left">
                      {{widget('companyname', 'companynamefooter', {"business": "profile"})|raw}}
                  </div>
                  <div class="footer-center">
                      {{widget('content', 'textbox2', {content: '<h4>Lorem Impsum</h4> <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Phasellus accumsan.</p>', lines: 'all'})|raw}}
                  </div>
                  <div class="footer-right">
                    {{widget('profile', 'footersocialicons', { 'profileType':'socialicons', 'socialicons':'profile', 'refreshtime':'1800000'})|raw}}
                  </div>
                </div>
      </footer>

{% endblock %}

